<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <!--
    https://docs.nuget.org/create/nuspec-reference#specifying-dependencies
    -->
	<metadata minClientVersion="2.8.1">
		<id>
			Xamarin.Auth.Custom
		</id>
		<!--
		SemVer - Semantic Versioning
		http://semver.org/
		-->
		<version>
			0.0.2
		</version>
		<title>
			Xamarin.Auth with UWP
		</title>
		<authors>
			blacknothing
		</authors>
		<owners>
			blacknothing
		</owners>
		<copyright>
			Copyright 2013-2016
		</copyright>
		<licenseUrl>
			https://github.com/blacknothing/Xamarin.Auth/blob/master/License.md
		</licenseUrl>
		<projectUrl>
			https://github.com/blacknothing/Xamarin.Auth
		</projectUrl>
		<requireLicenseAcceptance>
			false
		</requireLicenseAcceptance>
		<description>
			A cross-platform API for authenticating users and storing their accounts.
		</description>
		<summary>
			Modified Xamarin.Auth
		</summary>
		<tags>
			xamarin,
			auth,
			UWP
			<!--
			universal windows platform, uwp, windows 10
			plugin for xamarin, plugin,
			xamarin.forms,
			-->
		</tags>
		<frameworkAssemblies>
			<frameworkAssembly
				assemblyName="System.Json"
				targetFramework="MonoTouch10"
				/>
			<frameworkAssembly
				assemblyName="System.Json"
				targetFramework="MonoAndroid10"
				/>
				<frameworkAssembly
					assemblyName="System.Json"
					targetFramework="Xamarin.iOS10"
					/>
		</frameworkAssemblies>
	</metadata>
	<files>
		<!--
		src
			on windows with \
			on macosx with /
		-->
		<!--
			Core - Portable
		-->
		<file
			src="source\output\pcl\Xamarin.Auth.dll"
			target="lib\portable-net45+wp8+wpa81+win8+MonoAndroid10+MonoTouch10+XamarinIOS10\Xamarin.Auth.dll"
			>
		</file>

		<!--
			Xamarin.Android
		-->
		<file
			src="source\output\android\Xamarin.Auth.dll"
			target="lib\MonoAndroid10\Xamarin.Auth.dll"
			>
		</file>

		<!--
			Xamarin.iOS
				Unified
		-->
		<file
			src="source\output\ios-unified\Xamarin.Auth.dll"
			target="lib\Xamarin.iOS10\Xamarin.Auth.dll"
			>
		</file>

		<!--
			Xamarin.iOS Classic
		-->
		<file
			src="source\output\ios\Xamarin.Auth.dll"
			target="lib\MonoTouch10\Xamarin.Auth.dll"
			>
		</file>

		<!--
			Windows Phone 8 Silverlight
		<file
			src="./source/output/wp80/Xamarin.Auth.dll"
			target="lib\wp8\Xamarin.Auth.dll"
			>
		</file>
		-->

		<!--
			Windows Phone 8.1 WinRT
		
		<file
			src="./source/output/wpa81/Xamarin.Auth.dll"
			target="lib\wpa81\Xamarin.Auth.dll"
			>
		</file>-->

		<!--
			Windows 8.1 WinRT
			Windows Store
		
		<file
			src="./source/output/win81/Xamarin.Auth.dll"
			target="lib\win81\Xamarin.Auth.dll"
			>
		</file>-->
		<!--
			Universal Windows Platform, UWP
			Windows 10
		-->
		<file
			src="source\output\uwp\Xamarin.Auth.dll"
			target="lib\uap10.0\Xamarin.Auth.dll"
			>
		</file>

	</files>
</package>
